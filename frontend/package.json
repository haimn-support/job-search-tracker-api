{
  "name": "interview-position-tracker-frontend",
  "version": "1.0.0",
  "description": "Frontend for Interview Position Tracker API",
  "private": true,
  "dependencies": {
    "@headlessui/react": "^1.7.14",
    "@heroicons/react": "^2.0.17",
    "@tanstack/react-query": "^4.29.0",
    "@tanstack/react-query-devtools": "^4.29.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.23",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "autoprefixer": "^10.4.14",
    "axios": "^1.4.0",
    "chart.js": "^4.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^2.30.0",
    "postcss": "^8.4.23",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-hot-toast": "^2.4.1",
    "react-router-dom": "^6.10.0",
    "react-scripts": "^5.0.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.3.2",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "HOST=0.0.0.0 craco start",
    "start:wsl": "HOST=0.0.0.0 PORT=3000 craco start",
    "build": "craco build",
    "build:analyze": "ANALYZE=true npm run build",
    "build:production": "GENERATE_SOURCEMAP=false craco build",
    "build:staging": "REACT_APP_ENV=staging craco build",
    "test": "react-scripts test --watchAll=false --passWithNoTests",
    "test:watch": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --watchAll=false --passWithNoTests",
    "test:ci": "CI=true react-scripts test --coverage --watchAll=false --passWithNoTests",
    "test:debug": "react-scripts --inspect-brk test --runInBand --no-cache",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .ts,.tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write src/**/*.{ts,tsx,js,jsx,json,css,md}",
    "format:check": "prettier --check src/**/*.{ts,tsx,js,jsx,json,css,md}",
    "type-check": "tsc --noEmit",
    "analyze": "npm run build && npx bundle-analyzer build/static/js/*.js",
    "analyze:detailed": "npm run build:analyze",
    "bundle-size": "npm run build && npx bundlesize",
    "optimize": "./scripts/build-optimize.sh all",
    "optimize:analyze": "./scripts/build-optimize.sh analyze",
    "optimize:check-size": "./scripts/build-optimize.sh check-size",
    "optimize:clean": "./scripts/build-optimize.sh clean",
    "dev": "npm run start:wsl",
    "preview": "npm run build && npx serve -s build",
    "preview:production": "npm run build:production && npx serve -s build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^7.1.0",
    "@tailwindcss/forms": "^0.5.3",
    "@testing-library/jest-dom": "^6.8.0",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "bundlesize": "^0.18.2",
    "compression-webpack-plugin": "^10.0.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest-axe": "^10.0.0",
    "jest-transform-css": "^6.0.3",
    "msw": "^2.11.3",
    "prettier": "^2.8.8",
    "terser-webpack-plugin": "^5.3.9",
    "webpack": "^5.88.2",
    "webpack-bundle-analyzer": "^4.9.1",
    "whatwg-fetch": "^3.6.20",
    "workbox-webpack-plugin": "^7.0.0",
    "workbox-window": "^7.0.0"
  },
  "proxy": "http://localhost:8000"
}
